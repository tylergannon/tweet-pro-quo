---
import { get_quotes } from '$lib/db';
import { quote_slug } from '$lib/routes';
import Quote from '$lib/quote.astro';

export function getStaticPaths() {
  return get_quotes().map((quote) => ({
    params: {
      quote_slug: quote_slug(quote),
    },
    props: quote,
  }));
}
---

<Quote {...Astro.props} />
